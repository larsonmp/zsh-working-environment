[user]
    name = Micah Larson
    email = larson.micah@gmail.com
[core]
    editor = vim
[color]
    status = auto
    branch = auto
    interactive = auto
    grep = auto
[alias]
    ls = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    lt = log --oneline --decorate --all
    head = "!git ls -10"
[push]
	default = simple
